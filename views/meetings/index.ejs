<%- include('../partials/header') %>
  <body>
    <%- include('../partials/nav-bar') %>
    <div class="bgi">
    <div class = all-meeting><table class="table table-striped table-dark">
    <thead>
      <tr>
        <th></th>
        <th>Contact</th>
        <th>Event</th>
        <th>Date-Time</th>
        <th>Location</th>
        <th>Notes</th>
      </tr>
    </thead>
    <tbody>
      <% meetings.forEach(function(m) { %>
      <tr>
        <td><form
          action="/meetings/<%= m.id %>?_method=DELETE"
          class="delete-form"
          method="POST"
        >
          <button id="removeBtn" type="submit">DELETE</button>
        </form></td>
        <td><%= m.contact %></td>
        <td><%= m.event %></td>
        <td><%= m.date %></td>
        <td><%= m.location %></td>
        <td><a href="/meetings/<%= m._id %>/notes">Notes</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table></div>
    </div>
    </div>
  

  

